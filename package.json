{
  "name": "tap-events-force",
  "version": "1.0.0",
  "description": "Events Force Tap for the Keg",
  "main": "index.js",
  "repository": "https://github.com/simpleviewinc/tap-events-force.git",
  "author": "Lance Tipton <lancetipton04@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "android": "cd node_modules/sv-keg; yarn android",
    "build:android": "cd node_modules/sv-keg; yarn build:android",
    "build:ios": "cd node_modules/sv-keg; yarn build:ios",
    "build:web": "cd node_modules/sv-keg; yarn build:web",
    "clean": "cd node_modules/sv-keg; yarn clean",
    "clean:full": "yarn clean; yarn clean:nm; yarn install",
    "clean:nm": "rimraf ./yarn.lock; rimraf ./package.lock; rimraf node_modules",
    "clean:rn": "yarn keg clean:rn",
    "ios": "cd node_modules/sv-keg; yarn ios",
    "start": "cd node_modules/sv-keg; expo start",
    "cp": "/bin/bash cp.sh",
    "setup": "yarn install; cd node_modules/sv-keg; yarn postinstall",
    "web": "cd node_modules/sv-keg; yarn web",
    "keg": "cd node_modules/sv-keg; yarn"
  },
  "dependencies": {
    "firebase": "^7.7.0",
    "sv-keg": "https://github.com/simpleviewinc/keg-core.git#master",
    "qr-reader": "git+https://github.com/mpcarolin/qr-code-scanner.git#master",
    "keg-components": "https://github.com/mpcarolin/keg-components.git#better-inputs"

  },
  "devDependencies": {
    "rimraf": "^3.0.1"
  }
}
